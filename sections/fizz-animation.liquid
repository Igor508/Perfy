/* OBJECTS */

.bubble {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;

  -webkit-box-shadow: 0 20px 30px rgba(0, 0, 0, 0.1),
    inset 0px 10px 30px 5px rgba(255, 255, 255, .10);
  -moz-box-shadow: 0 20px 30px rgba(0, 0, 0, 0.1),
    inset 0px 10px 30px 5px rgba(255, 255, 255, .10);
  box-shadow: 0 20px 30px rgba(0, 0, 0, 0.1),
    inset 0px 10px 30px 5px rgba(255, 255, 255, .10);

  height: 50px;
  position: absolute;
  width: 50px;

  z-index: 0;
}

.bubble:after {
  background: -moz-radial-gradient(
    center,
    ellipse cover,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0) 5%
  ); /* FF3.6+ */
  background: -webkit-gradient(
    radial,
    center center,
    0px,
    center center,
    100%,
    color-stop(0%, rgba(255, 255, 255, 0.1)),
    color-stop(5%, rgba(255, 255, 255, 0))
  ); /* Chrome,Safari4+ */
  background: -webkit-radial-gradient(
    center,
    ellipse cover,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0) 5%%
  ); /* Chrome10+,Safari5.1+ */
  background: -o-radial-gradient(
    center,
    ellipse cover,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0) 5%
  ); /* Opera 12+ */
  background: -ms-radial-gradient(
    center,
    ellipse cover,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0) 5%
  ); /* IE10+ */
  background: radial-gradient(
    ellipse at center,
    rgba(255, 255, 255, 0.1) 0%,
    rgba(255, 255, 255, 0) 5%
  ); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient(
      startColorstr="#80ffffff",
      endColorstr="#00ffffff",
      GradientType=1
    ); /* IE6-9 fallback on horizontal gradient */

  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;

  -webkit-box-shadow: inset 0 20px 30px rgba(255, 255, 255, 0.10);
  -moz-box-shadow: inset 0 20px 30px rgba(255, 255, 255, 0.10);
  box-shadow: inset 0 20px 30px rgba(255, 255, 255, 0.10);

  content: "";
  height: 45px;
  left: 5px;
  position: absolute;
  width: 45px;
  z-index: 0;
}

/* KEYFRAMES */

@-webkit-keyframes animateBubble {
  0% {
    margin-top: 50px;
  }
  100% {
    margin-top: -40%;
  }
}

@-moz-keyframes animateBubble {
  0% {
    margin-top: 50px;
  }
  100% {
    margin-top: -40%;
  }
}

@keyframes animateBubble {
  0% {
    margin-top: 50px;
  }
  100% {
    margin-top: -40%;
  }
}

@media (prefers-reduced-motion) {
  .bubble {
    display: none;
  }
}
